using System;

namespace Assets.Scripts.GameSystem
{
	public class CTextImageNode
	{
		public string content;

		public bool bText;

		public float width;

		public float height;

		public float posX;

		public float posY;

		public CTextImageNode(string ct, bool bText, float width, float height, float x, float y)
		{
			this.content = ct;
			this.bText = bText;
			this.width = width;
			this.height = height;
			this.posX = x;
			this.posY = y;
		}
	}
}
