namespace MsgPack
{
    using System;
    using System.Text;

    public sealed class PackingOptions
    {
        private Encoding _stringEncoding = MessagePackConvert.Utf8NonBom;

        public Encoding StringEncoding
        {
            get
            {
                return this._stringEncoding;
            }
            set
            {
                this._stringEncoding = value;
            }
        }
    }
}

